{
  "categories": [
    {
      "title": "## Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## Bug Fixes",
      "labels": ["bug", "fix"]
    },
    {
      "title": "## Documentation",
      "labels": ["documentation"]
    },
    {
      "title": "## Other Changes",
      "labels": []
    }
  ],
  "template": "#{{CHANGELOG}}\n\n**Full Changelog**: #{{RELEASE_DIFF}}",
  "pr_template": "- #{{TITLE}} (#{{NUMBER}})",
  "empty_template": "No changes.",
  "transformers": [],
  "label_extractor": [
    {
      "pattern": "^(feat|feature)",
      "target": "$1",
      "on_property": "title"
    },
    {
      "pattern": "^(fix|bug)",
      "target": "$1",
      "on_property": "title"
    }
  ]
}
